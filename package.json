{
  "name": "recylocal",
  "version": "1.0.0",
  "scripts": {
    "start": "node server.js",
    "dev:client": "cd client && vite",
    "build:client": "cd client && vite build",
    "dev": "concurrently \"npm run start\" \"npm run dev:client\"",
    "build": "npm run build:client"
  },
  "dependencies": {
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@mui/material": "^5.0.0",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0"
  },
  "devDependencies": {
    "vite": "^2.6.0",
    "concurrently": "^6.0.2"
  }
}
